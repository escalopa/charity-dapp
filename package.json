  "scripts": {
    "clean": "rimraf artifacts cache coverage build coverage.json",
    "load": "yarn install && npx hardhat compile && truffle compile",
    "docs": "npx hardhat docgen",
    "cov-loc": "npx hardhat coverage --network localhost",
    "before:commit": "yarn lint && yarn test && yarn docs",
    "lint": "yarn run lint:js && yarn run lint:sol",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:sol:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:sol": "solhint --max-warnings 150 \"contracts/**/*.sol\"",
    "lint:sol:fix": "solhint --max-warnings 150 \"contracts/**/*.sol\" --fix",
    "coverage": "hardhat coverage",
    "deploy": "hardhat deploy --network",
    "deploy:test": "hardhat deploy",
    "test": "hardhat test"
  },
